' Input: A number
' Output: Counting till inputted number
' E.g. Input Number: 7
Imports System.Console
Module Module1

    Sub Main()
    Dim n, m As Integer
    Write("Please enter a number: ")
    n = ReadLine()
    m = 0

program:
    WriteLine(m)
    m = m + 1
    If m <= n Then GoTo program

    End Sub
End Module